
function sf() {
  echo
  printf "\e[1;33m%6d  %s\e[0m\n" $(git log --oneline | wc -l ) "Total Commits"
  echo "   ---  -------------"
  git shortlog -se | sed -e 's/[^0-9]*</ /' -e 's/@sep.com>//' | awk '{ printf "%6d  %s\n", $1, $2 }' | sort -gr
  printf "\n\n"
  printf "%6d  %s\n" $(grep -r "\@Test" FCUMobile/test/ | wc -l) "Unit Tests"
  printf "%6d  %s\n" $(grep -r "\@\w*Test" FCUMobileInstrumentationTests | wc -l) "Instrumentation Tests"
  printf "\n\n"
  printf "%6d  %s\n" $(grep -r "^[[:space:]]*[[:alpha:]]* class" FCUMobile/src/ | wc -l) "App classes"
  printf "%6d  %s\n" $(grep -r "^[[:space:]]*[[:alpha:]]* class" FCUMobile/test/ | wc -l) "Unit test classes"
  printf "%6d  %s\n" $(grep -r "^[[:space:]]*[[:alpha:]]* class" FCUMobileInstrumentationTests/src/ | wc -l) "Instrumentation test classes"
  echo
}